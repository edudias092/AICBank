{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "Using": ["Serilog.Sinks.Grafana.Loki"],
    "MinimumLevel": {
      "Default": "Debug"
    },
    "WriteTo":[{
      "Name": "GrafanaLoki",
      "Args": {
        "uri": "http://localhost:3100",
        "labels":[
          {"key": "app", "value":"aicbank_api"}
        ]
      }
    }]
  },
  "CelCash":{
    "galaxId": "#{CelCash_galaxId}#",
    "galaxHash": "#{CelCash_galaxHash}#",
    "baseUrl": "#{CelCash_baseUrl}#"
  },
  "ConnectionStrings": {
    "AICBankDbConnString": "#{ConnectionStrings_AICBankDbConnString}#"
  },
  "JWT": {
    "TokenValidityInMinutes": "#{JWT_TokenValidityInMinutes}#",
    "SecretKey": "#{JWT_SecretKey}#",
    "Issuer": "#{JWT_Issuer}#",
    "Audience": "#{JWT_Audience}#"
  },
  "Email": {
    "Smtp": "#{Email_Smtp}#",
    "Port": "#{Email_Port}#",
    "User": "#{Email_User}#",
    "Password": "#{Email_Password}#"
  }
}
